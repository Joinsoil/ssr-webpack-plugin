{
  "name": "ssr-webpack-plugin",
  "version": "1.1.0",
  "description": "webpack plugin for generating a bundle manifest to use with Vue 2.x bundleRenderer or react-ssr run-in-vm",
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "build": "babel src --presets=@babel/env,@babel/stage-0 -d lib",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JounQin/ssr-webpack-plugin.git"
  },
  "keywords": ["react", "vue", "ssr", "webpack", "plugin"],
  "author": "JounQin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JounQin/ssr-webpack-plugin/issues"
  },
  "homepage": "https://github.com/JounQin/ssr-webpack-plugin#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "hash-sum": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-stage-0": "^7.0.0-beta.35"
  }
}
